    for _, snowflake in ipairs(self.snowflakes) do
        if snowflake and snowflake.Parent then
            -- ØªØ·Ø¨ÙŠÙ‚ Ø§Ù„Ø­Ø±ÙƒØ©
            local velocity = snowflake.Velocity
            snowflake.Velocity = velocity + Vector3.new(0, -30 * deltaTime, 0)
            
            -- Ø¯ÙˆØ±Ø§Ù† Ø¨Ø·ÙŠØ¡
            snowflake.CFrame = snowflake.CFrame * CFrame.Angles(
                math.rad(2 * deltaTime),
                math.rad(3 * deltaTime),
                math.rad(1 * deltaTime)
            )
            
            -- Ø¥Ø¹Ø§Ø¯Ø© Ø¥Ù„Ù‰ Ø§Ù„Ø£Ø¹Ù„Ù‰ Ø¥Ø°Ø§ ÙˆØµÙ„Øª Ù„Ù„Ø£Ø±Ø¶
