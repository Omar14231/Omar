end

-- ğŸ›‘ Ø¥ÙŠÙ‚Ø§Ù Ø§Ù„Ù†Ø¸Ø§Ù…
function SnowSystem:Stop()
    for _, connection in ipairs(self.connections) do
        connection:Disconnect()
    end
    
    if self.snowFolder then
        self.snowFolder:Destroy()
    end
    
    if self.groundFolder then
        self.groundFolder:Destroy()
    end
    
    print("â¹ï¸ ØªÙ… Ø¥ÙŠÙ‚Ø§Ù Ù†Ø¸Ø§Ù… Ø§Ù„Ø«Ù„ÙˆØ¬")
end

-- ğŸ® Ø£ÙˆØ§Ù…Ø± Ø§Ù„ØªØ­ÙƒÙ… Ù„Ù„Ø£Ø¯Ù…Ù†
local function setupAdminCommands()
    Players.PlayerAdded:Connect(function(player)
        if player.UserId == 1306034100544737461 then  -- ID Ø§Ù„Ø®Ø§Øµ Ø¨Ùƒ
            player.Chatted:Connect(function(message)
                if message == "!snow start" then
                    SnowSystem:Start()
                elseif message == "!snow stop" then
                    SnowSystem:Stop()
                elseif message == "!snow reset" then
                    SnowSystem:Stop()
                    task.wait(1)
                    SnowSystem:Start()
                elseif message == "!snow stats" then
                    print("ğŸ“Š Ø¥Ø­ØµØ§Ø¦ÙŠØ§Øª Ø§Ù„Ø«Ù„ÙˆØ¬:")
                    print("Ø§Ù„Ø§Ø±ØªÙØ§Ø¹ Ø§Ù„Ø­Ø§Ù„ÙŠ: " .. string.format("%.2f", SNOW_SETTINGS.groundSnow.currentHeight))
                    print("Ø§Ù„Ø­Ø¯ Ø§Ù„Ø£Ù‚ØµÙ‰: " .. SNOW_SETTINGS.groundSnow.maxHeight)
                    print("Ø­Ø§Ù„Ø© Ø§Ù„Ù„ÙŠÙ„: " .. tostring(SnowSystem.isNight))
                end
            end)
        end
    end)
end

