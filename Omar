                snowflake.Position = Vector3.new(
                    math.random(-SNOW_SETTINGS.snowflakes.areaSize.X/2, SNOW_SETTINGS.snowflakes.areaSize.X/2),
                    SNOW_SETTINGS.snowflakes.respawnHeight + math.random(0, 20),
                    math.random(-SNOW_SETTINGS.snowflakes.areaSize.Z/2, SNOW_SETTINGS.snowflakes.areaSize.Z/2)
                )
                
                snowflake.Velocity = Vector3.new(
                    SNOW_SETTINGS.snowflakes.wind.X + math.random(-2, 2),
                    SNOW_SETTINGS.snowflakes.fallSpeed.Y,
                    SNOW_SETTINGS.snowflakes.wind.Z + math.random(-2, 2)
                )
            end
        end
    end
end

-- â±ï¸ ØªÙˆÙ‚ÙŠØª ØªÙƒØ±Ø§Ø± ØµÙˆØª Ø§Ù„Ø«Ù„Ø¬
function SnowSystem:StartSoundLoop()
    local lastSoundTime = 0
    
    local soundConnection = RunService.Heartbeat:Connect(function()
        local currentTime = tick()
        
        if currentTime - lastSoundTime >= SOUNDS.snowfallDuration then
            self:PlaySnowfallSound()
            lastSoundTime = currentTime
        end
    end)
    
    table.insert(self.connections, soundConnection)
end

-- ğŸš¶ ØªØªØ¨Ø¹ Ø­Ø±ÙƒØ© Ø§Ù„Ù„Ø§Ø¹Ø¨ÙŠÙ† Ù„Ù„ØµÙˆØª
function SnowSystem:TrackPlayerFootsteps()
    local lastUpdate = {}
    
    local footstepConnection = RunService.Heartbeat:Connect(function(deltaTime)
        for _, player in ipairs(Players:GetPlayers()) do
            if player.Character then
                local humanoid = player.Character:FindFirstChild("Humanoid")
                local rootPart = player.Character:FindFirstChild("HumanoidRootPart")
                
                if humanoid and rootPart and humanoid:GetState() == Enum.HumanoidStateType.Running then
                    -- Ø§Ù„ØªØ­Ù‚Ù‚ Ù…Ù† Ø§Ù„ÙˆÙ‚ÙˆÙ Ø¹Ù„Ù‰ Ø§Ù„Ø«Ù„Ø¬
                    local rayOrigin = rootPart.Position
                    local rayDirection = Vector3.new(0, -5, 0)
                    local raycastParams = RaycastParams.new()
                    raycastParams.FilterDescendantsInstances = {Workspace.GroundSnow}
                    raycastParams.FilterType = Enum.RaycastFilterType.Include
                    
                    local raycastResult = Workspace:Raycast(rayOrigin, rayDirection, raycastParams)
                    
                    if raycastResult then
                        -- ØªØ´ØºÙŠÙ„ ØµÙˆØª Ø§Ù„Ø®Ø·ÙˆØ§Øª Ù…Ø¹ ØªÙˆÙ‚ÙŠØª Ù…Ù†Ø§Ø³Ø¨
                        local now = tick()
                        if not lastUpdate[player.UserId] or (now - lastUpdate[player.UserId]) > 0.6 then
                            self:PlayFootstepSound(player)
                            lastUpdate[player.UserId] = now
                        end
                    end
                end
            end
        end
    end)
    
    table.insert(self.connections, footstepConnection)
end

