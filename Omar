-- ğŸ”Š ØªØ´ØºÙŠÙ„ ØµÙˆØª ØªØ³Ø§Ù‚Ø· Ø§Ù„Ø«Ù„Ø¬
function SnowSystem:PlaySnowfallSound()
    if self.sounds.snowfall and self.sounds.snowfall.IsPlaying then
        return
    end
    
    local sound = Instance.new("Sound")
    sound.SoundId = "rbxassetid://" .. SOUNDS.snowfall
    sound.Volume = SOUNDS.snowfallVolume
    sound.Looped = false
    sound.Parent = SoundService
    
    sound:Play()
    
    -- Ø­Ø°Ù Ø§Ù„ØµÙˆØª Ø¨Ø¹Ø¯ Ø§Ù†ØªÙ‡Ø§Ø¦Ù‡
    game:GetService("Debris"):AddItem(sound, 30)
    
    self.sounds.snowfall = sound
    print("â„ï¸ ØªØ´ØºÙŠÙ„ ØµÙˆØª ØªØ³Ø§Ù‚Ø· Ø§Ù„Ø«Ù„Ø¬")
end

-- ğŸ‘£ ØªØ´ØºÙŠÙ„ ØµÙˆØª Ø§Ù„Ø®Ø·Ùˆ Ø¹Ù„Ù‰ Ø§Ù„Ø«Ù„Ø¬ Ù„Ù„Ø§Ø¹Ø¨
function SnowSystem:PlayFootstepSound(player)
    if not player.Character then return end
    
    local humanoid = player.Character:FindFirstChild("Humanoid")
    if not humanoid or humanoid:GetState() ~= Enum.HumanoidStateType.Running then
        return
    end
    
    -- Ø§Ù„ØªØ­Ù‚Ù‚ Ù…Ù† Ø§Ù„ÙˆÙ‚Øª Ø¨ÙŠÙ† Ø§Ù„Ø®Ø·ÙˆØ§Øª Ù„Ù…Ù†Ø¹ Ø§Ù„ØªÙƒØ±Ø§Ø± Ø§Ù„Ø³Ø±ÙŠØ¹
    local lastStep = self.lastStepTime or {}
    local currentTime = tick()
    
    if lastStep[player.UserId] and (currentTime - lastStep[player.UserId]) < 0.5 then
        return
    end
    
    lastStep[player.UserId] = currentTime
    self.lastStepTime = lastStep
    
    -- ØªØ´ØºÙŠÙ„ Ø§Ù„ØµÙˆØª
    local sound = Instance.new("Sound")
    sound.SoundId = "rbxassetid://" .. SOUNDS.footstep
    sound.Volume = SOUNDS.footstepVolume
    sound.Parent = player.Character:FindFirstChild("HumanoidRootPart") or player.Character.PrimaryPart
    
    sound:Play()
    
    game:GetService("Debris"):AddItem(sound, 2)
end

-- â„ï¸ Ø¥Ù†Ø´Ø§Ø¡ Ø­Ø¨ÙŠØ¨Ø§Øª Ø§Ù„Ø«Ù„Ø¬ Ø§Ù„Ù…ØªØ³Ø§Ù‚Ø·Ø©
function SnowSystem:CreateSnowflakes()
    self.snowFolder = Instance.new("Folder")
    self.snowFolder.Name = "SnowfallSystem"
    self.snowFolder.Parent = Workspace
    
    for i = 1, SNOW_SETTINGS.snowflakes.count do
        local snowflake = Instance.new("Part")
        snowflake.Name = "Snowflake_" .. i
        snowflake.Size = Vector3.new(
            math.random(SNOW_SETTINGS.snowflakes.minSize * 10, SNOW_SETTINGS.snowflakes.maxSize * 10) / 10,
            math.random(SNOW_SETTINGS.snowflakes.minSize * 10, SNOW_SETTINGS.snowflakes.maxSize * 10) / 10,
            math.random(SNOW_SETTINGS.snowflakes.minSize * 10, SNOW_SETTINGS.snowflakes.maxSize * 10) / 10
        )
        
        snowflake.Shape = Enum.PartType.Ball
        snowflake.Material = Enum.Material.Ice
        snowflake.Color = Color3.fromRGB(255, 255, 255)
        snowflake.Transparency = math.random(20, 40) / 100
        snowflake.CanCollide = false
        snowflake.Anchored = false
        snowflake.CastShadow = false
        snowflake.Massless = true
        
        -- ÙˆØ¶Ø¹ Ø¹Ø´ÙˆØ§Ø¦ÙŠ ÙÙŠ Ø§Ù„Ø³Ù…Ø§Ø¡
        snowflake.Position = Vector3.new(
            math.random(-SNOW_SETTINGS.snowflakes.areaSize.X/2, SNOW_SETTINGS.snowflakes.areaSize.X/2),
            SNOW_SETTINGS.snowflakes.respawnHeight + math.random(0, 20),
            math.random(-SNOW_SETTINGS.snowflakes.areaSize.Z/2, SNOW_SETTINGS.snowflakes.areaSize.Z/2)
        )
        
        -- Ø³Ø±Ø¹Ø© Ø³Ù‚ÙˆØ· Ø¹Ø´ÙˆØ§Ø¦ÙŠØ©
        snowflake.Velocity = Vector3.new(
            SNOW_SETTINGS.snowflakes.wind.X + math.random(-2, 2),
            SNOW_SETTINGS.snowflakes.fallSpeed.Y,
            SNOW_SETTINGS.snowflakes.wind.Z + math.random(-2, 2)
        )
        
        snowflake.Parent = self.snowFolder
        table.insert(self.snowflakes, snowflake)
    end
end

-- ğŸ—» Ø¥Ù†Ø´Ø§Ø¡ Ø§Ù„Ø«Ù„ÙˆØ¬ Ø§Ù„Ù…ØªØ±Ø§ÙƒÙ…Ø© Ø¹Ù„Ù‰ Ø§Ù„Ø£Ø±Ø¶
function SnowSystem:CreateGroundSnow()
    local groundFolder = Instance.new("Folder")
    groundFolder.Name = "GroundSnow"
    groundFolder.Parent = Workspace
    
    -- Ø¥Ù†Ø´Ø§Ø¡ Ø´Ø¨ÙƒØ© Ù…Ù† Ø§Ù„Ø«Ù„ÙˆØ¬ Ø¹Ù„Ù‰ Ø§Ù„Ø£Ø±Ø¶
