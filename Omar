    local cellSize = 8
    
    for x = -gridSize, gridSize do
        for z = -gridSize, gridSize do
            local snowPatch = Instance.new("Part")
            snowPatch.Name = "SnowPatch_" .. x .. "_" .. z
            snowPatch.Size = Vector3.new(cellSize, 0.1, cellSize)
            snowPatch.Position = Vector3.new(x * cellSize, 0, z * cellSize)
            snowPatch.Color = SNOW_SETTINGS.groundSnow.snowColor
            snowPatch.Material = SNOW_SETTINGS.groundSnow.material
            snowPatch.TopSurface = Enum.SurfaceType.Smooth
            snowPatch.BottomSurface = Enum.SurfaceType.Smooth
            snowPatch.Anchored = true
            snowPatch.CanCollide = true
            snowPatch.Transparency = 0
            
            -- ØªØ¨Ø§ÙŠÙ† ÙÙŠ Ø§Ù„Ø§Ø±ØªÙØ§Ø¹ Ù„ØªØ¨Ø¯Ùˆ Ø·Ø¨ÙŠØ¹ÙŠØ©
            local yOffset = math.random(-5, 5) / 100
            snowPatch.Position = Vector3.new(snowPatch.Position.X, yOffset, snowPatch.Position.Z)
            
            snowPatch.Parent = groundFolder
            table.insert(self.groundSnow, snowPatch)
        end
    end
    
    self.groundFolder = groundFolder
end

-- ğŸ“ˆ ØªØ±Ø§ÙƒÙ… Ø§Ù„Ø«Ù„ÙˆØ¬ Ø¹Ù„Ù‰ Ø§Ù„Ø£Ø±Ø¶ ØªØ¯Ø±ÙŠØ¬ÙŠØ§Ù‹
function SnowSystem:AccumulateSnow(deltaTime)
    if SNOW_SETTINGS.groundSnow.currentHeight >= SNOW_SETTINGS.groundSnow.maxHeight then
        return
    end
    
    -- Ø²ÙŠØ§Ø¯Ø© Ø§Ø±ØªÙØ§Ø¹ Ø§Ù„Ø«Ù„Ø¬ ØªØ¯Ø±ÙŠØ¬ÙŠØ§Ù‹
    SNOW_SETTINGS.groundSnow.currentHeight = SNOW_SETTINGS.groundSnow.currentHeight + 
        (SNOW_SETTINGS.groundSnow.accumulationRate * deltaTime)
    
    -- ØªØ­Ø¯ÙŠØ« Ø§Ø±ØªÙØ§Ø¹ ÙƒÙ„ Ø±Ù‚Ø¹Ø© Ø«Ù„Ø¬
    for _, snowPatch in ipairs(self.groundSnow) do
        if snowPatch and snowPatch.Parent then
            local currentPos = snowPatch.Position
            local targetY = math.min(
                currentPos.Y + (SNOW_SETTINGS.groundSnow.accumulationRate * deltaTime * 0.5),
                SNOW_SETTINGS.groundSnow.maxHeight / 2
            )
            
            snowPatch.Position = Vector3.new(currentPos.X, targetY, currentPos.Z)
            snowPatch.Size = Vector3.new(snowPatch.Size.X, 0.1 + targetY * 2, snowPatch.Size.Z)
        end
    end
end

-- ğŸŒ™ ØªØ­ÙˆÙŠÙ„ Ø§Ù„Ø¬Ùˆ Ø¥Ù„Ù‰ Ù„ÙŠÙ„ ØªØ¯Ø±ÙŠØ¬ÙŠ
function SnowSystem:TransitionToNight(deltaTime)
    if not SNOW_SETTINGS.nightEffects.enabled or self.isNight then
        return
    end
    
    -- ØªØºÙŠÙŠØ± Ø§Ù„Ø¥Ø¶Ø§Ø¡Ø© ØªØ¯Ø±ÙŠØ¬ÙŠØ§Ù‹
    local current = SNOW_SETTINGS.nightEffects.currentAmbient
    local target = SNOW_SETTINGS.nightEffects.targetAmbient
    
    local newColor = Color3.new(
        current.R + (target.R - current.R) * SNOW_SETTINGS.nightEffects.transitionSpeed,
        current.G + (target.G - current.G) * SNOW_SETTINGS.nightEffects.transitionSpeed,
        current.B + (target.B - current.B) * SNOW_SETTINGS.nightEffects.transitionSpeed
    )
    
    SNOW_SETTINGS.nightEffects.currentAmbient = newColor
    Lighting.Ambient = newColor
    
    -- Ø¥Ø¶Ø§ÙØ© Ø¶Ø¨Ø§Ø¨
    if Lighting.FogEnd > 200 then
        Lighting.FogEnd = Lighting.FogEnd - deltaTime * 10
        Lighting.FogColor = Color3.fromRGB(150, 180, 210)
    end
    
    -- Ø§Ù„ØªØ­Ù‚Ù‚ Ø¥Ø°Ø§ ÙˆØµÙ„Ù†Ø§ Ù„Ù„Ù„ÙŠÙ„ Ø§Ù„ÙƒØ§Ù…Ù„
    if math.abs(newColor.R - target.R) < 0.05 and
       math.abs(newColor.G - target.G) < 0.05 and
       math.abs(newColor.B - target.B) < 0.05 then
        self.isNight = true
        print("ğŸŒ™ ØªØ­ÙˆÙ„ Ø§Ù„Ø¬Ùˆ Ø¥Ù„Ù‰ Ù„ÙŠÙ„ ÙƒØ§Ù…Ù„")
    end
end

-- ğŸ”„ ØªØ­Ø¯ÙŠØ« Ø­Ø±ÙƒØ© Ø§Ù„Ø«Ù„ÙˆØ¬ Ø§Ù„Ù…ØªØ³Ø§Ù‚Ø·Ø©
function SnowSystem:UpdateSnowflakes(deltaTime)
